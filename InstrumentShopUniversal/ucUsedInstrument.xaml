<UserControl
    x:Class="InstrumentShopUniversal.ucUsedInstrument"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:InstrumentShopUniversal"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400">

    <Grid>
        <TextBlock x:Name="txbCondition" HorizontalAlignment="Left" Margin="20,22,0,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="18"/>
        <TextBlock x:Name="txbReturnDate" HorizontalAlignment="Left" Margin="20,64,0,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="18"/>

    </Grid>
</UserControl>
